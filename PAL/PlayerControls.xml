					<posy>0</posy>
					<width>46</width>
					<height>46</height>					
					<onleft>13</onleft>
					<onright>15</onright>
				    <texturefocus border="1">osd-rew-focus.png</texturefocus>
			        <texturenofocus border="1">osd-rew.png</texturenofocus>
					<onclick>XBMC.PlayerControl(Rewind)</onclick>
				</control>

				<control type="button" id="15">
					<posx>45</posx>
					<posy>-10</posy>
					<width>56</width>
					<height>66</height>
					<onleft>16</onleft>
					<onright>18</onright>
					<texturefocus border="2">osd-pause-focus.png</texturefocus>
					<texturenofocus border="2">osd-pause.png</texturenofocus>
					<onclick>XBMC.PlayerControl(Play)</onclick>
					<visible>!Player.Paused</visible>
					<!-- <visible>!Player.Paused | !Player.Rewinding | !Player.Forwarding | !Player.Seeking</visible> -->
