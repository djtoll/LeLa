					<texturenofocus border="1">osd-next.png</texturenofocus>
					<onclick>XBMC.PlayerControl(Next)</onclick>
				</control>
			</control>

			<control type="group">
				<posx>710</posx>
				<posy>55</posy>
				<control type="label" id="22">
					<description>seek time</description>
					<posx>0</posx>
					<posy>7</posy>
					<font>cali38</font>
					<align>right</align>
					<label>[B]$INFO[Player.Time] / [COLOR=sublabel-text]$INFO[Player.Duration][COLOR][/B]</label>
					<textcolor>label-text</textcolor>
					<width>300</width>
					<visible>Player.Playing</visible>
				</control>
			</control>

		</control>	






