<window id="139">
	<defaultcontrol>5</defaultcontrol>
	
	<include>dialog-open</include>
	
	<coordinates>
		<system>1</system>
		<posx>100</posx>
		<posy>80</posy>
	</coordinates>
	
	<controls>
		
	<control type="group">	
		<control type="image">
			<posx>-30</posx>
			<posy>-30</posy>
			<width>400</width>
			<height>490</height>
			<texture border="38">dialogpanel.png</texture>
		</control>
		<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>800</width>
			<height>610</height>
			<texture border="40">DialogFront.png</texture>
		</control>
		<control type="label" id="1">
			<description>heading label</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>345</width>
			<height>30</height>
			<align>left</align>
			<aligny>center</aligny>
			<font>h1</font>
			<label>$LOCALIZE[13406]</label>
		</control>

		<control type="list" id="5">
			<posx>0</posx>
			<posy>50</posy>
			<width>345</width>
			<height>378</height>
			<onleft>5</onleft>
			<onright>5</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<pagecontrol>-</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout height="63">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>340</width>
					<height>60</height>
					<texture border="2">list-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>0</posy>
					<width>320</width>
					<height>35</height>
					<font>menu</font>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>25</posy>
					<width>320</width>
					<height>35</height>
					<font>menu</font>
					<aligny>center</aligny>
					<align>left</align>
					<textcolor>sublabel-text</textcolor>
					<info>ListItem.Label2</info>
				</control>
			</itemlayout>
			<focusedlayout height="63">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>340</width>
					<height>60</height>
					<visible>!Control.HasFocus(5)</visible>
					<texture border="2">list-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>340</width>
					<height>60</height>
					<visible>Control.HasFocus(5)</visible>
					<texture border="2">list-focus.png</texture>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>0</posy>
					<width>320</width>
					<height>35</height>
					<font>menu</font>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Label</info>
					<scroll>true</scroll>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>25</posy>
					<width>320</width>
					<height>35</height>
					<font>menu</font>
					<aligny>center</aligny>
					<align>left</align>
					<textcolor>sublabel-text</textcolor>
					<info>ListItem.Label2</info>
					<scroll>true</scroll>
				</control>
			</focusedlayout>
		</control>
		<control type="label">
			<description>number of files/pages in list text label</description>
			<posx>750</posx>
			<posy>535</posy>
			<width>700</width>
			<font>font12b</font>
			<align>right</align>
			<scroll>true</scroll>
			<textcolor>white</textcolor>
			<label>($INFO[Container(5).NumItems]) $LOCALIZE[31025] - $LOCALIZE[31024] ($INFO[Container(5).CurrentPage]/$INFO[Container(5).NumPages])</label>
		</control>
	</control>
	</controls>
</window>