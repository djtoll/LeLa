<window id="2005">
	<defaultcontrol>-</defaultcontrol>
	<controls>
		
		

	<control type="group" id="1">
		<posx>-10</posx>
		<posy>380</posy>
		<visible>Player.ShowInfo + !Window.IsActive(2901)</visible>
		
		<control type="image" id="1">
			<posx>-10</posx>
			<posy>63</posy>
			<width>760</width>
			<height>150</height>
			<texture border="0,0,22,0">osd-infobar.png</texture>
			<include>fadeinout-osd</include>
			<visible>Player.ShowInfo</visible>
		</control>

		
		<animation type="Visible">
			<effect type="slide" start="0,70" end="0" time="100" />
			<effect type="fade" start="0" end="80" time="100" />
		</animation>
		<animation type="Hidden">
			<effect type="slide" start="0,0" end="0,70" time="100" />
			<effect type="fade" start="80" end="0" time="100" />
		</animation>

		
<!---------- MOVIES --------------------------------------------------------------------------------------->
		
		<control type="group" id="1">
			<visible>videoplayer.content(movies)</visible>
			
			<control type="image" id="1">
				<description>cover image</description>
				<posx>25</posx>
				<posy>95</posy>
				<width>100</width>
				<height>85</height>
				<info>videoplayer.cover</info>
				<bordertexture border="2">list-focus.png</bordertexture>
				<bordersize>2</bordersize>
				<aspectratio align="left">keep</aspectratio>
				<visible>videoplayer.content(movies)</visible>
			</control>
			
			<control type="label" id="1">
				<description>Movie Title</description>
				<width>420</width>
				<posx>0</posx>
				<posy>95</posy>
				<info>VideoPlayer.Title</info>
				<align>left</align>
				<font>h1</font>
				<visible>videoplayer.content(movies)</visible>
				<animation effect="slide" start="0" end="90" time="0" condition="!IsEmpty(videoplayer.cover)">conditional</animation>
			</control>
			
			<control type="label" id="1">
				<description>Movie Genre</description>
				<width>520</width>
				<posx>0</posx>
				<posy>126</posy>
				<info>VideoPlayer.Genre</info>
				<align>left</align>
				<font>h2</font>
				<textcolor>sublabel-text</textcolor>
				<animation effect="slide" start="0" end="90" time="0" condition="!IsEmpty(videoplayer.cover)">conditional</animation>
			</control>
			
			<control type="label" id="1">
				<description>Progress</description>
				<width>520</width>
				<posx>0</posx>
				<posy>154</posy>
				<label>$INFO[player.Time]$INFO[player.Duration, / ]</label>
				<align>left</align>
				<font>h2</font>
				<textcolor>sublabel-text</textcolor>
				<animation effect="slide" start="0" end="90" time="0" condition="!IsEmpty(videoplayer.cover)">conditional</animation>
			</control>

			<control type="label" id="1">
				<description>FINISH time label</description>
				<width>310</width>
				<posx>710</posx>
				<posy>153</posy>
				<label>$LOCALIZE[31068]</label>	
				<align>right</align>
				<font>h4</font>
				<textcolor>sublabel-text</textcolor>
			</control>
			
			<control type="label" id="1">
				<description>Chapter Pos No</description>
				<width>310</width>
				<posx>0</posx>
				<posy>166</posy>
				<label>$INFO[player.chapter,$LOCALIZE[21396] ]$INFO[player.chaptercount, / ]</label>
				<visible>player.chaptercount</visible>
				<align>right</align>
				<font>h2</font>
				<animation effect="slide" start="0" end="90" time="0" condition="!IsEmpty(videoplayer.cover)">conditional</animation>
			</control>
		
		</control>
			
			
