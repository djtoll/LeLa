<window id="104">
	<defaultcontrol>1</defaultcontrol>
	
	<coordinates>
		<system>1</system>
		<posx>530</posx>
		<posy>70</posy>
	</coordinates>
	
	<controls>

		<control type="group">
			<include>fadeinout</include>
			
			<control type="image">
				<description>background image</description>
				<posx>-30</posx>
				<posy>-23</posy>
				<width>225</width>
				<height>65</height>
				<texture border="28">keyboardPanel.png</texture>	
			</control>
			
			<control type="progress">
				<description>progress control</description>
				<posx>-4</posx>
				<posy>3</posy>
				<width>100</width>
				<height>13</height>
				<info>Player.Volume</info>
				<texturebg>progress_back.png</texturebg>
				<lefttexture>-</lefttexture>
				<midtexture>progress-middle.png</midtexture>
				<righttexture>-</righttexture>
				<overlaytexture>-</overlaytexture>
				<visible>!Window.IsActive(113)</visible>
			</control>

			<control type="label" id="0">
				<description>Volume Label</description>
				<posx>110</posx>
				<posy>-2</posy>
				<font>h3</font>
				<align>left</align>
				<info>Player.Volume</info>
				<width>80</width>
				<textcolor>label-text</textcolor>
				<!--<visible>!Window.IsActive(113)</visible>-->
			</control>
			
		</control>
	</controls>
</window>
